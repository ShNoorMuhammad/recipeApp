<%= render 'shared/page_title', title: "Chef #{@chef.chefname}'s Profile" %>

<div class="row">
  <div class="col-md-4 col-md-offset-4 center">
  <div align="center">
    <%= gravatar_for @chef, size: 150 %>
    </div>
  </div>
</div>
<% if @chef.items.count != 0 %>
  <h4 align="center"> <%= @chef.chefname %>'s recipe  </h4>
  <h1><%=session[:chef_id] %></h1>


  <div align="center">
  <%= will_paginate @chef_items %>
  </div>

  <%= render 'items/item' ,obj: @chef_items %>
  <div align="center">
  <%= will_paginate @chef_items %>
  </div>
<%else %>
<h4 align="center"> No recipe found !!!!!  </h4>
<%end%>


